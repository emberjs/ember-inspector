{
  "name": "ember-inspector",
  "version": "4.2.0",
  "description": "Extends developer tools to allow you to better inspect your Ember.js apps.",
  "repository": "https://github.com/emberjs/ember-inspector",
  "license": "MIT",
  "author": "Tilde, Inc.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "build:production": "EMBER_ENV=production node scripts/download-panes.js && ember build --environment production && gulp compress:chrome && gulp compress:firefox && gulp clean-tmp",
    "changelog": "github_changelog_generator -u emberjs -p ember-inspector --since-tag v3.8.0",
    "compress:panes": "gulp compress:chrome-pane && gulp compress:firefox-pane && gulp compress:bookmarklet-pane",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lock-version": "yarn build:production && yarn compress:panes && EMBER_ENV=production node scripts/upload-panes.js",
    "serve:bookmarklet": "ember serve --port 9191",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "COVERAGE=true ember test",
    "upload:panes": "yarn build && yarn compress:panes && node scripts/upload-panes.js",
    "watch": "ember build --watch"
  },
  "devDependencies": {
    "@babel/helper-call-delegate": "^7.10.4",
    "@ember/edition-utils": "^1.2.0",
    "@ember/optional-features": "^1.3.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "@html-next/vertical-collection": "^1.0.0",
    "amd-name-resolver": "^1.3.1",
    "aws-sdk": "^2.731.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.0",
    "basiccontext": "^3.5.1",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-babel-transpiler": "^7.7.0",
    "broccoli-concat": "^4.2.4",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "broccoli-string-replace": "^0.1.2",
    "codeclimate-test-reporter": "^0.5.1",
    "compare-versions": "^3.5.0",
    "del": "^5.1.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.20.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-code-coverage": "^1.0.0-beta.9",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-showdown": "^4.5.0",
    "ember-cli-stylelint": "^4.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-cli-version-checker": "^5.1.1",
    "ember-concurrency": "^1.2.1",
    "ember-did-resize-modifier": "^1.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-flatpickr": "^2.16.4",
    "ember-load-initializers": "^2.1.1",
    "ember-math-helpers": "^2.15.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.20.4",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.2.3",
    "ember-table": "^2.2.3",
    "ember-template-lint": "^2.10.0",
    "ember-test-selectors": "^4.1.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.1.0",
    "ensure-posix-path": "^1.1.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^8.10.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fstream": "^1.0.12",
    "got": "^11.5.2",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.2",
    "loader.js": "^4.7.0",
    "normalize.css": "8.0.1",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.3",
    "prettier": "^2.0.5",
    "qunit-dom": "^1.4.0",
    "release-it": "^13.6.5",
    "release-it-lerna-changelog": "^2.3.0",
    "rimraf": "^3.0.0",
    "sass": "^1.26.10",
    "stylelint": "^13.6.1",
    "stylelint-config-ship-shape": "^0.6.0",
    "yauzl": "^2.10.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      "lib/ui"
    ]
  },
  "emberVersionsSupported": [
    "3.4.0",
    ""
  ],
  "previousEmberVersionsSupported": [
    "0.0.0",
    "2.7.0"
  ],
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": false,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    }
  },
  "volta": {
    "node": "10.19.0",
    "yarn": "1.22.0"
  }
}
