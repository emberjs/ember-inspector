{
  "name": "ember-inspector",
  "version": "3.11.0",
  "description": "Extends developer tools to allow you to better inspect your Ember.js apps.",
  "license": "MIT",
  "author": "Tilde, Inc.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/emberjs/ember-inspector",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "COVERAGE=true ember test",
    "watch": "ember build --watch",
    "serve:bookmarklet": "ember serve --port 9191",
    "build:production": "EMBER_ENV=production node scripts/download-panes.js && ember build --environment production && gulp compress:chrome && gulp compress:firefox && gulp clean-tmp",
    "compress:panes": "gulp compress:chrome-pane && gulp compress:firefox-pane && gulp compress:bookmarklet-pane",
    "upload:panes": "yarn build && yarn compress:panes && node scripts/upload-panes.js",
    "lock-version": "yarn build:production && yarn compress:panes && EMBER_ENV=production node scripts/upload-panes.js"
  },
  "emberVersionsSupported": [
    "3.4.0",
    ""
  ],
  "previousEmberVersionsSupported": [
    "0.0.0",
    "2.7.0"
  ],
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "@html-next/vertical-collection": "^1.0.0-beta.13",
    "amd-name-resolver": "^1.2.1",
    "aws-sdk": "^2.480.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.0",
    "basiccontext": "^3.5.1",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-babel-transpiler": "^6.5.1",
    "broccoli-concat": "^3.7.3",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.2",
    "broccoli-stew": "^3.0.0",
    "broccoli-string-replace": "^0.1.1",
    "codeclimate-test-reporter": "^0.5.1",
    "compare-versions": "^3.5.0",
    "del": "^4.1.1",
    "ember-cli": "~3.12.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-moment-shim": "^3.7.1",
    "ember-cli-pretender": "^3.1.1",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-showdown": "^4.4.4",
    "ember-cli-stylelint": "^3.0.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^2.1.0",
    "ember-cli-version-checker": "^3.1.3",
    "ember-concurrency": "^1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^6.5.1",
    "ember-flatpickr": "^2.14.0",
    "ember-load-initializers": "^2.0.0",
    "ember-math-helpers": "^2.11.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.12.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-svg-jar": "^1.2.2",
    "ember-table": "^2.1.0",
    "ember-test-selectors": "^2.1.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.1.0",
    "eslint-plugin-ember": "^6.3.0",
    "eslint-plugin-node": "^9.0.1",
    "fstream": "^1.0.8",
    "gulp": "^4.0.2",
    "gulp-zip": "^4.2.0",
    "js-string-escape": "^1.0.0",
    "loader.js": "^4.7.0",
    "mkdirp": "^0.5.1",
    "qunit-dom": "^0.8.4",
    "rimraf": "^2.5.2",
    "sass": "^1.21.0",
    "stylelint": "^10.1.0",
    "stylelint-config-ship-shape": "^0.6.0",
    "yauzl": "^2.10.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "paths": [
      "lib/ui"
    ]
  }
}
