<EmberTable as |t|>
  <t.head @columns={{columns}} @enableReorder={{false}} />

  <t.body @rows={{rows}} as |b|>
    <b.row
      class={{
        concat "js-config-row" (if (mod b.rowMeta.index 2) " striped")
      }} as |r|
    >
      <r.cell class={{concat "js-config-" r.columnValue.valuePath}} as |value|>
        {{value}}
      </r.cell>
    </b.row>
  </t.body>
</EmberTable>