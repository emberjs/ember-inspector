<div class="list-view">
  <div class="list-view__header row">
    <div class="cell cell_type_header cell_size_large">
      Library
    </div>
    <div class="cell cell_type_header">
      Version
    </div>
    <!-- Account for scrollbar width :'(  -->
    <div class="cell cell_type_header spacer"></div>
  </div>
  <div class="list-view__list-container">
    <div class="list-tree">
      <div class="ember-list-container">
        {{#each model as |library|}}
          <div class="list-tree__item-wrapper row-wrapper js-library-row">
            <div class="list-tree__item row">
              <div class="cell_type_main cell cell_size_large">
                <span class="js-lib-name">{{unbound library.name}}</span>
              </div>
              <div class="cell">
                <span class="js-lib-version">{{unbound library.version}}</span>
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>
