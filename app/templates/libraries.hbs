<EmberTable as |t|>
  <t.head
    @columns={{schema-for "info-list"}}
    @enableReorder={{false}}
  />

  <t.body @rows={{rows}} as |b|>
    <b.row
      class={{concat "js-library-row" (if (mod b.rowMeta.index 2) " striped")}}
    as |r|
    >
      <r.cell
        class={{concat "js-lib-" r.columnValue.valuePath}}
      as |value|
      >
        {{value}}
      </r.cell>
    </b.row>
  </t.body>
</EmberTable>
